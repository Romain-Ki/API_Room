generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Room {
  id        Int      @id @default(autoincrement())
  name      String
  capacity  Int
  features  Json
  rules     Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
